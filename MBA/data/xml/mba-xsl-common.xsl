<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:fo="http://www.w3.org/1999/XSL/Format">
<!-- http://www.w3.org/TR/xslt -->

<xsl:variable name="BODY">font-family:verdana; font-size:10px; color:black; background-color:black;</xsl:variable>
<xsl:variable name="TABLE">position:relative; border:1px solid black;table-layout:auto; width:auto;</xsl:variable>
<xsl:variable name="TH">vertical-align:top; padding:3;border-bottom:1 solid black; background-color:gray; color:silver; text-align:left; font-size:11px;</xsl:variable>
<xsl:variable name="TD">white-space:no-wrap;padding:3 5 3 5; border-bottom:1 solid black; background-color:#EEEEEE; font-size:10px;</xsl:variable>
<xsl:variable name="SELECT">background-color: #EEEEEE;border: 1px solid #000000;color: #000000;font: 11px Arial;</xsl:variable>

</xsl:stylesheet>